{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "test": "vitest",
    "test:watch": "vitest",
    "test:run": "vitest --run",
    "prepare": "husky install",
    "postinstall": "nuxt prepare",
    "postbuild": "node ./scripts/gen-meta.mjs && node ./scripts/gen-meta.mjs --check-only",
    "check:meta": "node scripts/gen-meta.mjs --check-only",
    "lh:quick": "pnpm dlx lighthouse https://migakiexplorer.jp --only-categories=accessibility,seo --preset=desktop --view",
    "lh:quick:desktop": "pnpm dlx lighthouse https://migakiexplorer.jp --only-categories=accessibility,seo --preset=desktop --quiet",
    "lh:quick:mobile": "pnpm dlx lighthouse https://migakiexplorer.jp --only-categories=accessibility,seo --preset=mobile --quiet",
    "smoke:og": "node scripts/smoke-og.mjs",
    "ci:local": "powershell -ExecutionPolicy Bypass -File ./scripts/ci-local.ps1",
    "test:api": "vitest run",
    "tag:auto": "powershell -ExecutionPolicy Bypass -File ./scripts/bump-tag.ps1 auto",
    "tag:patch": "powershell -ExecutionPolicy Bypass -File ./scripts/bump-tag.ps1 patch",
    "tag:minor": "powershell -ExecutionPolicy Bypass -File ./scripts/bump-tag.ps1 minor",
    "tag:major": "powershell -ExecutionPolicy Bypass -File ./scripts/bump-tag.ps1 major"
  },
  "dependencies": {
    "@nuxt/content": "3.6.3",
    "nuxt": "^4.1.1",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#content/server": "@nuxt/content/nitro"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@nuxtjs/sitemap": "latest",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/node": "^20.19.14",
    "@types/react": "^19.1.13",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "10",
    "better-sqlite3": "^12.2.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.6",
    "eslint-plugin-vue": "^9.28.0",
    "h3": "^1.15.4",
    "jsdom": "^27.0.0",
    "postcss": "8",
    "react": "^19.1.1",
    "supertest": "^7.0.0",
    "tailwindcss": "3.4.10",
    "typescript-eslint": "^8.8.0",
    "vitest": "^3.2.4",
    "vue-tsc": "3.0.7",
    "zod": "3.24.1",
    "zod-to-json-schema": "3.24.6"
  },
  "pnpm": {
    "overrides": {
      "zod": "3.24.1",
      "zod-to-json-schema": "3.24.6",
      "@nuxt/content": "3.6.3"
    }
  }
}
