{
  "ci": {
    "collect": {
      "settings": {
        "preset": "desktop"
      },
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.9 }],
        "categories:accessibility": ["error", { "minScore": 0.9 }],
        "categories:best-practices": ["error", { "minScore": 0.96 }],
        "categories:seo": ["error", { "minScore": 1 }]
      }
    }
  }
}
